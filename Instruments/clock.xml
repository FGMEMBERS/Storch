<?xml version="1.0"?>

<PropertyList>

<path>clock.ac</path>

    <animation>
        <type>material</type>
        <object-name>Clock.001</object-name>
        <object-name>Hourhand</object-name>
        <object-name>Minutehand</object-name>
        <object-name>Secondhand</object-name>
        <emission>
            <red>0.5</red>
            <green>0.2</green>
            <blue>0.0</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Hourhand</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
    <factor>0.008333</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Minutehand</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
    <factor>0.1</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Secondhand</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
    <factor>6</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>